{
	"[toml]": {
		"editor.defaultFormatter": "tamasfe.even-better-toml"
	},
	"chat.agent.enabled": true,
	"chat.agent.maxRequests": 128,
	"chat.agent.thinkingStyle": "collapsedPreview",
	"chat.extensionUnification.enabled": true,
	"chat.mcp.gallery.enabled": true,
	"chat.tools.terminal.outputLocation": "none",
	"coderabbit.autoReviewMode": "prompt",
	"diffEditor.experimental.showMoves": true,
	"diffEditor.hideUnchangedRegions.enabled": true,
	"diffEditor.ignoreTrimWhitespace": false,
	"editor.accessibilitySupport": "off",
	"editor.aiStats.enabled": true,
	"editor.autoClosingComments": "never",
	"editor.autoClosingDelete": "always",
	"editor.autoClosingOvertype": "always",
	"editor.autoIndent": "advanced",
	"editor.codeActionsOnSave": [
		"source.fixAll.ts",
		"source.fixAll.eslint",
		"source.fixAll.stylelint",
		"source.fixAll.biome",
		"source.organizeImports.ts",
		"source.organizeImports.biome",
		"source.action.useSortedAttributes.biome",
		"source.action.useSortedKeys.biome",
		"source.action.useSortedProperties.biome"
	],
	"editor.defaultFormatter": "prettier.prettier-vscode",
	"editor.find.cursorMoveOnType": false,
	"editor.find.seedSearchStringFromSelection": "selection",
	"editor.fontFamily": "'FiraCode Nerd Font', Menlo, Monaco, 'Courier New', monospace",
	"editor.fontLigatures": true,
	"editor.formatOnSave": true,
	"editor.guides.bracketPairs": "active",
	"editor.inlayHints.enabled": "offUnlessPressed",
	"editor.inlineSuggest.experimental.showOnSuggestConflict": "whenSuggestListIsIncomplete",
	"editor.inlineSuggest.showToolbar": "always",
	"editor.linkedEditing": true,
	"editor.maxTokenizationLineLength": 1000,
	"editor.minimap.showSlider": "always",
	"editor.occurrencesHighlight": "multiFile",
	"editor.parameterHints.cycle": false,
	"editor.parameterHints.enabled": false,
	"editor.renderWhitespace": "all",
	"editor.scrollBeyondLastLine": false,
	"editor.snippetSuggestions": "bottom",
	"editor.stickyScroll.enabled": true,
	"editor.suggest.localityBonus": true,
	"editor.suggest.preview": true,
	"editor.suggest.selectionMode": "whenTriggerCharacter",
	"editor.tabSize": 2,
	"emmet.showExpandedAbbreviation": "inMarkupAndStylesheetFilesOnly",
	"explorer.confirmUndo": "verbose",
	"explorer.incrementalNaming": "smart",
	"files.associations": {
		".dev.vars{,.*}": "ini",
		".env{,.*}": "ini",
		"**/.husky/*": "shellscript"
	},
	"files.eol": "\n",
	"files.readonlyInclude": {
		"**/node_modules/**": true
	},
	"git.autofetch": "all",
	"git.closeDiffOnOperation": true,
	"git.diagnosticsCommitHook.enabled": true,
	"git.fetchOnPull": true,
	"git.mergeEditor": true,
	"git.pruneOnFetch": true,
	"git.verboseCommit": true,
	"github.copilot.chat.editor.temporalContext.enabled": true,
	"github.copilot.chat.edits.temporalContext.enabled": true,
	"github.copilot.chat.generateTests.codeLens": true,
	"github.copilot.chat.languageContext.typescript.enabled": true,
	"github.copilot.chat.localeOverride": "ja",
	"github.copilot.nextEditSuggestions.enabled": true,
	"github.copilot.nextEditSuggestions.fixes": true,
	"githubPullRequests.codingAgent.uiIntegration": true,
	"inlineChat.enableV2": true,
	"javascript.inlayHints.functionLikeReturnTypes.enabled": true,
	"javascript.inlayHints.parameterNames.enabled": "all",
	"javascript.inlayHints.parameterTypes.enabled": true,
	"javascript.inlayHints.propertyDeclarationTypes.enabled": true,
	"javascript.inlayHints.variableTypes.enabled": true,
	"prettier.singleQuote": true,
	"prettier.useTabs": true,
	"redhat.telemetry.enabled": true,
	"search.showLineNumbers": true,
	"terminal.explorerKind": "external",
	"terminal.external.osxExec": "Warp.app",
	"terminal.sourceControlRepositoriesKind": "external",
	"typescript.enablePromptUseWorkspaceTsdk": true,
	"typescript.inlayHints.functionLikeReturnTypes.enabled": true,
	"typescript.inlayHints.parameterNames.enabled": "all",
	"typescript.inlayHints.parameterTypes.enabled": true,
	"typescript.inlayHints.propertyDeclarationTypes.enabled": true,
	"typescript.inlayHints.variableTypes.enabled": true,
	"typescript.preferences.autoImportSpecifierExcludeRegexes": ["^console$"],
	"typescript.preferences.preferTypeOnlyAutoImports": true,
	"window.autoDetectColorScheme": true,
	"window.confirmBeforeClose": "keyboardOnly",
	"window.title": "${rootName}${separator}${activeFolderMedium}",
	"workbench.commandPalette.experimental.enableNaturalLanguageSearch": false,
	"workbench.editor.alwaysShowEditorActions": true,
	"workbench.editor.pinnedTabSizing": "compact",
	"workbench.editor.showTabIndex": true,
	"workbench.editorAssociations": {
		"*.svg": "default"
	},
	"workbench.preferredDarkColorTheme": "GitHub Dark Default",
	"workbench.preferredLightColorTheme": "GitHub Light Default",
	"workbench.settings.enableNaturalLanguageSearch": false,
	"workbench.tree.enableStickyScroll": true
}
