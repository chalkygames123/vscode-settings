{
	"[html]": {
		"editor.formatOnSave": false
	},
	"diffEditor.experimental.showMoves": true,
	"diffEditor.experimental.useVersion2": true,
	"diffEditor.hideUnchangedRegions.enabled": true,
	"editor.acceptSuggestionOnEnter": "smart",
	"editor.accessibilitySupport": "off",
	"editor.autoClosingDelete": "always",
	"editor.autoIndent": "advanced",
	"editor.codeActionsOnSave": {
		"source.addMissingImports": "explicit",
		"source.fixAll.eslint": "explicit",
		"source.fixAll.stylelint": "explicit"
	},
	"editor.cursorSurroundingLines": 7,
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.find.seedSearchStringFromSelection": "selection",
	"editor.fontFamily": "'FiraCode Nerd Font', Menlo, Monaco, 'Courier New', monospace",
	"editor.fontLigatures": true,
	"editor.formatOnSave": true,
	"editor.guides.bracketPairs": true,
	"editor.inlayHints.enabled": "offUnlessPressed",
	"editor.inlineSuggest.suppressSuggestions": true,
	"editor.insertSpaces": false,
	"editor.linkedEditing": true,
	"editor.maxTokenizationLineLength": 1000,
	"editor.minimap.showSlider": "always",
	"editor.occurrencesHighlight": "multiFile",
	"editor.renderWhitespace": "all",
	"editor.stickyScroll.enabled": true,
	"editor.suggest.preview": true,
	"editor.suggestSelection": "recentlyUsed",
	"editor.tabSize": 2,
	"emmet.triggerExpansionOnTab": true,
	"explorer.incrementalNaming": "smart",
	"files.associations": {
		"*.njk": "html"
	},
	"files.eol": "\n",
	"files.trimFinalNewlines": true,
	"git.autofetch": "all",
	"git.closeDiffOnOperation": true,
	"git.fetchOnPull": true,
	"git.mergeEditor": true,
	"git.pruneOnFetch": true,
	"github.copilot.enable": {
		"*": true,
		"markdown": true,
		"plaintext": true,
		"scminput": true
	},
	"japanese-proofreading.textlint.丸かっこ（）": false,
	"japanese-proofreading.textlint.全角文字と半角文字の間": false,
	"japanese-proofreading.textlint.大かっこ［］": false,
	"javascript.inlayHints.functionLikeReturnTypes.enabled": true,
	"javascript.inlayHints.parameterNames.enabled": "all",
	"javascript.inlayHints.parameterTypes.enabled": true,
	"javascript.inlayHints.propertyDeclarationTypes.enabled": true,
	"javascript.inlayHints.variableTypes.enabled": true,
	"prettier.singleQuote": true,
	"prettier.useTabs": true,
	"redhat.telemetry.enabled": true,
	"scm.experimental.showSyncView": true,
	"search.searchOnType": false,
	"terminal.integrated.env.osx": {
		"FIG_NEW_SESSION": "1"
	},
	"typescript.inlayHints.functionLikeReturnTypes.enabled": true,
	"typescript.inlayHints.parameterNames.enabled": "all",
	"typescript.inlayHints.parameterTypes.enabled": true,
	"typescript.inlayHints.propertyDeclarationTypes.enabled": true,
	"typescript.inlayHints.variableTypes.enabled": true,
	"window.autoDetectColorScheme": true,
	"window.confirmBeforeClose": "keyboardOnly",
	"window.enableMenuBarMnemonics": false,
	"window.menuBarVisibility": "hidden",
	"window.title": "${activeEditorMedium}${separator}${rootName}",
	"workbench.colorTheme": "GitHub Light",
	"workbench.editor.doubleClickTabToToggleEditorGroupSizes": "maximize",
	"workbench.editor.tabSizing": "fixed",
	"workbench.preferredDarkColorTheme": "GitHub Dark",
	"workbench.preferredLightColorTheme": "GitHub Light",
	"workbench.startupEditor": "none"
}
