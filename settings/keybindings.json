// Place your key bindings in this file to override the defaults
[
	{
		"key": "alt+cmd+u",
		"command": "cursorRedo"
	},
	{
		"key": "ctrl+f12",
		"command": "editor.action.goToTypeDefinition"
	},
	{
		"key": "tab",
		"command": "editor.action.inlineSuggest.commit",
		"when": "inlineSuggestionHasIndentationLessThanTabSize && inlineSuggestionVisible && !editorHoverFocused && !editorTabMovesFocus"
	},
	{
		"key": "tab",
		"command": "-editor.action.inlineSuggest.commit",
		"when": "inlineSuggestionHasIndentationLessThanTabSize && inlineSuggestionVisible && !editorHoverFocused && !editorTabMovesFocus && !suggestWidgetVisible"
	},
	{
		"key": "ctrl+cmd+.",
		"command": "editor.action.sourceAction"
	},
	{
		"key": "ctrl+shift+alt+d",
		"command": "editor.emmet.action.balanceIn"
	},
	{
		"key": "ctrl+alt+d",
		"command": "editor.emmet.action.balanceOut"
	},
	{
		"key": "ctrl+alt+y",
		"command": "editor.emmet.action.evaluateMathExpression"
	},
	{
		"key": "ctrl+alt+t",
		"command": "editor.emmet.action.matchTag"
	},
	{
		"key": "ctrl+shift+cmd+g",
		"command": "gitlens.views.commitDetails.focus"
	},
	{
		"key": "cmd+i",
		"command": "-inlineChat.start",
		"when": "editorFocus && inlineChatHasProvider && inlineChatPossible && !editorReadonly"
	},
	{
		"key": "ctrl+k g",
		"command": "-textPowerTools.filterLinesIncludingString",
		"when": "editorTextFocus"
	},
	{
		"key": "ctrl+cmd+w",
		"command": "workbench.action.closeEditorsToTheRight"
	},
	{
		"key": "ctrl+shift+pageup",
		"command": "workbench.action.moveEditorLeftInGroup"
	},
	{
		"key": "ctrl+shift+pagedown",
		"command": "workbench.action.moveEditorRightInGroup"
	},
	{
		"key": "ctrl+shift+[Minus]",
		"command": "workbench.action.navigateForward"
	},
	{
		"key": "cmd+r r",
		"command": "workbench.action.tasks.reRunTask"
	},
	{
		"key": "cmd+r shift+r",
		"command": "workbench.action.tasks.runTask"
	},
	{
		"key": "cmd+r shift+x",
		"command": "workbench.action.tasks.terminate"
	},
	{
		"key": "cmd+r t",
		"command": "workbench.action.tasks.test"
	},
	{
		"key": "ctrl+cmd+j",
		"command": "workbench.action.toggleMaximizedPanel"
	},
	{
		"key": "shift+cmd+0",
		"command": "workbench.action.zoomReset"
	},
	{
		"key": "cmd+numpad0",
		"command": "-workbench.action.zoomReset"
	}
]
